{
  "defaultProvider": "openrouter",
  "skills": [
    "~/.config/agents/skills"
  ],
  "prompts": [
    "~/.config/agents/prompts"
  ],
  "extensions": [
    "~/.pi/agent/extensions/sub-agents",
    "~/.pi/agent/extensions/editor.ts",
    "~/.pi/agent/extensions/handoff.ts",
    "~/.pi/agent/extensions/session-name.ts"
  ],
  "compaction": {
    "enabled": false
  },
  "lastChangelogVersion": "0.53.0",
  "defaultModel": "kimi-k2-thinking",
  "defaultThinkingLevel": "medium",
  "hideThinkingBlock": true,
  "steeringMode": "all",
  "followUpMode": "all",
  "collapseChangelog": true,
  "doubleEscapeAction": "tree"
}
